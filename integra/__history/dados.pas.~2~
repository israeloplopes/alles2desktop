{ %CLASSGROUP 'Vcl.Controls.TControl' }

uses principal, verificainternet;

{$R *.dfm}

procedure Tdm_DADOS.Memoria;
var
  MainHandle : THandle;
begin
  try
    MainHandle := OpenProcess(PROCESS_ALL_ACCESS, false, GetCurrentProcessID) ;
    SetProcessWorkingSetSize(MainHandle, $FFFFFFFF, $FFFFFFFF) ;
    CloseHandle(MainHandle) ;
  except
  end;
  Application.ProcessMessages;
end;

procedure Tdm_DADOS.ExecutaSGINICONSP;
begin
  zqry_sgconexao.close;
  zqry_sgconexao.open;
  zqry_SGINICONSP.Params.Clear;
  zqry_Sginiconsp.Close;
  zqry_SGINICONSP.SQL.Clear;
  zqry_sginiconsp.SQL.text:='execute procedure sginiconsp(:empresa, :usuario, :filial, :conexao)';
  zqry_sginiconsp.ParamByName('empresa').AsInteger:=dm_Dados.zqry_SGCONEXAOCODEMP.AsInteger;
  zqry_sginiconsp.ParamByName('filial').AsInteger:=dm_Dados.zqry_SGCONEXAOCODFILIAL.AsInteger;
  zqry_sginiconsp.ParamByName('usuario').value:=dm_Dados.zqry_SGCONEXAOUSER.value;
  zqry_sginiconsp.ParamByName('conexao').AsInteger:=dm_Dados.zqry_SGCONEXAOCURRENT_CONNECTION.AsInteger;
  zqry_SGINICONSP.Open;
end;

procedure Tdm_DADOS.DataModuleCreate(Sender: TObject);
var
   WEBSERVER, WEBNAME, WEBBD :STRING;
   TEMINTERNET:integer;
begin
  //Le o arquivo INI e carrega os valores nas respectivas variveis
   ini         := TIniFile.Create('..\ini\freedom.ini');
   g_Data_Base := ini.ReadString('configuracao', 'database', '');
   s_Path_NFe  := ini.ReadString('configuracao', 'pathnfe', '');
   usuario := ini.ReadString('configuracao','userbd','');
   senha := ini.ReadString('configuracao','senhabd','');
   wallpaper:=ini.ReadString('configuracao','wallpaper','');
   wp_integra:=ini.ReadString('configuracao','wp_integra','');
   wp_checkout:=ini.ReadString('configuracao','wp_checkout','');
   logoloja:=ini.ReadString('configuracao','logoloja','');
   logodanfe:=ini.ReadString('configuracao','logodanfe','');
   logomarca:=ini.ReadString('configuracao','logomarca','');
   ambiente:=strtoint(ini.ReadString('configuracao','ambiente',''));
   host:=ini.ReadString('configuracao','host','');
   bdsys:=ini.ReadString('configuracao','bdsys','');
   codemp:=strtoint(ini.readstring('configuracao','codemp',''));
   estacao:=strtoint(ini.ReadString('configuracao','numterm',''));
   pedidobranco:=ini.ReadString('configuracao','pedidobranco','');
   impressao50:=ini.ReadString('configuracao','Impressao50mm','');
   tefestacao:=ini.ReadString('configuracao','tefestacao','');
   contingencia:=ini.ReadString('configuracao','contingencia','');
   if contingencia='' then contingencia:='N';
   adicnf:=ini.ReadString('configuracao','adicionalnfce','');
   imprimemonitor:=ini.ReadString('configuracao','imprimemonitor','');
   if imprimemonitor='' then imprimemonitor:='N';
   imprimeprotocolo:=ini.ReadString('configuracao','imprimeprotocolo','');
   if imprimeprotocolo='' then imprimeprotocolo:='S';
   largurabobina:=strtoint(ini.readstring('configuracao','LarguraBobina',''));
   distancia:=strtoint(ini.readstring('configuracao','DistanciaIntegra',''));
   if distancia=0 then distancia:=50;
   usuariodobd:=ini.ReadString('configuracao','usuariotst','');
   senhadobd:=ini.ReadString('configuracao','senhatst','');
   //showmessage(inttostr(largurabobina));
   //ms:=strtofloat(ini.ReadString('configuracao','ms',''));
   //mi:=strtofloat(ini.ReadString('configuracao','mi',''));
   //md:=strtofloat(ini.ReadString('configuracao','md',''));
   //me:=strtofloat(ini.ReadString('configuracao','me',''));


 //  showmessage(inttostr(frm_principal.posicaox));
   if largurabobina=0 then largurabobina:=302;
   //if mi=0 then mi:=0.5;
   //if ms=0 then ms:=0.80;
   //if me=0 then me:=0.60;
   //if md=0 then md:=0.51;



   //frm_principal.lbl_wallpaper.caption:=wallpaper;

   wpimg0:=ini.ReadString('configuracao','img_wp1','');
   wpimg1:=ini.ReadString('configuracao','img_wp2','');
   wpimg2:=ini.ReadString('configuracao','img_wp3','');
   wpimg3:=ini.ReadString('configuracao','img_wp4','');
   wpimg4:=ini.ReadString('configuracao','img_wp5','');
   ini.free;

   // ZEOS
  dm_DADOS.zCONECTA.Connected       := False;
  dm_DADOS.zCONECTA.HostName        := host;
  dm_DADOS.zCONECTA.Port            := 3050;
  //        dm_DADOS.zCONECTA.Protocol        := Ini.ReadString('Conexao_ZEOS', 'Protocol', '');
  //        dm_DADOS.zCONECTA.LibraryLocation := Ini.ReadString('Conexao_ZEOS', 'LibraryLocation', '');
  dm_DADOS.zCONECTA.User            := usuario;
  dm_DADOS.zCONECTA.Password        := senha;
  dm_DADOS.zCONECTA.Database        := bdsys;

  s_NFe_Ent := s_Path_NFe + '\Ent\Entnfe.txt';
  s_NFe_Resp := s_Path_NFe + '\Sai\Entnfe-resp.txt';
  g_Msg := 'Erro ao conectar com a base de dados.' + #13 + 'Data Base: ' + g_Data_Base + #13 + 'No Localizada';

  try
    dm_DADOS.zCONECTA.connected:=true;
  except
    on E:Exception do
    MessageDlg('Erro ao conectar!'#13'Erro: ' + e.Message, mtError, [mbOK], 0);
  end;

  try
    teminternet:=verificainternet.Ping(10000,'www.nonoelemento.com.br',80);
    if teminternet=0 then dm_dados.MySQLConectar.Connected:=true;
  except
  on E: Exception do
    showmessage(E.ClassName+' Dificuldades de conexo com a Nuvem : '+E.Message);
  end;
end;

procedure Tdm_DADOS.zqry_EQCULTURAAfterPost(DataSet: TDataSet);
begin
zqry_EQCULTURA.ApplyUpdates;
end;

procedure Tdm_DADOS.zqry_VDCLIENTENewRecord(DataSet: TDataSet);
var
 new_codcli:integer;
begin
new_codcli:=0;
zqry_VDCODCLI.Close;
zqry_VDCODCLI.SQL.Clear;
zqry_VDCODCLI.sql.add('SELECT MAX(codcli) as Sequencia FROM vdcliente');
zqry_VDCODCLI.Open;
new_codcli:=1+zqry_vdcodcli.FieldByname('sequencia').asInteger;
zqry_VDCLIENTECODCLI.AsInteger:=new_codcli;
end;

end.

