    { Private declarations }
  public
    { Public declarations }
    // INTEGER
    temnet,estacao,maqadm,codemp,porta, sync, largura,crypt,xmlsign,httplib,SSLLib,ssltype,
    timeout, partner, numvias:integer;
    // STRING
    hostweb,bancoweb,usuarioweb,host,bdsys,bdintegra,banco,usuario,senha,logo,
    caminhopfx,senhapfx,seriepfx,impressao50, pedidobranco,ticket,logomarca,empresa,meuusuario,
    ambiente, idtoken, csctoken,ufambiente, alerta,aguardar, tentativas, intervalo,
    pathnfe, pathsalvar,pathschemas, pathevento, caminhonfe, caminhopdf,caminhoboleto, hostmail, portmail,
    xsslmail, usermail, passmail, emitenfce, usasat, usuariost,senhast, nativo, fusohorario, bdmenuto, hostmenuto,
    nomecupom, formaspagtonfce,tefestacao,numtef,enderecoip,loja, terminal,sincrono, vendedornota,autorizado2:string;
    // BOOLEAN
    atualizaxml,erroschema,retiraacento,salvarxml :boolean;

  end;

var
  dm_DADOS: Tdm_DADOS;
  ini: TIniFile;

implementation

{%CLASSGROUP 'Vcl.Controls.TControl'}

uses u_verificainternet;

{$R *.dfm}

procedure Tdm_DADOS.datamodule(Sender: TObject);
var
  testastr:string;
begin
  // tem internet
  temnet:=u_verificainternet.Ping(10000,'www.google.com.br',80);
  //Le o arquivo INI e carrega os valores nas respectivas variveis
   ini         := TIniFile.Create('..\ini\alles.ini');
  //////////////////////////////////////////////////////////////////////////////
  // Web
  //////////////////////////////////////////////////////////////////////////////
  hostweb:=     ini.ReadString('nonoelemento','webserver','');
  bancoweb:=    'nonoelem_'+ini.readstring('nonoelemento','webbd','');
  usuarioweb:=  ini.ReadString('nonoelemento','webname','')+'_israel';
  partner:=     ini.ReadInteger('nonoelemento','partner',0);
  //////////////////////////////////////////////////////////////////////////////
  // local
  //////////////////////////////////////////////////////////////////////////////
  host:=     ini.ReadString('dados','host','');
  bdsys:=    ini.ReadString('dados','bdsys','');
  porta:=    strtoint(ini.readstring('dados','porta',''));
  banco:=    ini.readstring('dados','banco','');
  usuario:=  ini.ReadString('dados','userbd','');
  senha :=   ini.ReadString('dados','passbd','');
  //////////////////////////////////////////////////////////////////////////////
  // menuto
  //////////////////////////////////////////////////////////////////////////////
  hostmenuto:=ini.ReadString('dados','hostmenuto','');
  bdmenuto:=ini.ReadString('dados','bdmenuto','');
  //////////////////////////////////////////////////////////////////////////////
  // estacao
  //////////////////////////////////////////////////////////////////////////////
  estacao:= strtoint(ini.ReadString('estacao','terminal',''));
  testastr:=emptystr;
  testastr:=ini.ReadString('estacao','maqadm','');
  if testastr=''  then testastr:='1';
  maqadm  :=  strtoint(testastr);
  if maqadm=0 then showmessage('Ateno'#13'Nmero da Mquina Administrativa no est configurado');
  usuariost:= ini.ReadString('estacao','usuariotst','');
  senhast:= ini.ReadString('estacao','senhatst','');
  //////////////////////////////////////////////////////////////////////////////
  // parametros
  //////////////////////////////////////////////////////////////////////////////
  sync := strtoint(ini.ReadString('opcoes','sync',''));
  caminhonfe := ini.ReadString('opcoes','caminhonfe','');
  caminhoboleto:=ini.readstring('opcoes','caminhoboleto','');
  nativo:=   ini.ReadString('opcoes','nativo','');
  tefestacao := ini.ReadString('opcoes','tefestacao','');
  sincrono :=ini.ReadString('opcoes','sincrono','');
  if sincrono='' then sincrono:='S';
  numtef :=ini.ReadString('opcoes','numtef','');
  enderecoip := ini.ReadString('opcoes','enderecoip','');
  loja := ini.ReadString('opcoes','loja','');
  terminal := ini.ReadString('opcoes','terminal','');
  codemp:=strtoint(ini.ReadString('opcoes','codemp',''));
  bdintegra :=ini.ReadString('opcoes','bdintegra','');
  vendedornota:=ini.ReadString('opcoes','vendedornota','');
  if vendedornota='' then vendedornota:='N';
  // Impressao
  impressao50   := ini.ReadString('opcoes','impressao50mm','');
  pedidobranco  := ini.ReadString('opcoes','pedidobranco','');
  if pedidobranco='' then pedidobranco:='N';
  ticket := ini.ReadString('opcoes','imprimeticket','');
  largura := strtoint(ini.ReadString('opcoes','largurabobina',''));
  emitenfce:=ini.ReadString('opcoes','emitenfce','');
  if emitenfce='' then emitenfce:='S';
  usasat:=ini.ReadString('opcoes','usasat','');
  if usasat='' then usasat:='N';
  nomecupom:=ini.ReadString('opcoes','nomecupom','');
  if nomecupom='' then nomecupom:='N';
  formaspagtonfce:=ini.ReadString('opcoes','formaspagtonfce','');
  if formaspagtonfce='' then formaspagtonfce:='N';
  numvias := StrToIntDef(ini.ReadString('parametros', 'numvias', ''), 0);
  if numvias = 0 then numvias := 1;
  //////////////////////////////////////////////////////////////////////////////
  // Aparencia
  //////////////////////////////////////////////////////////////////////////////
  logomarca   := ini.ReadString('aparencia','logomarca','');
  empresa  := ini.ReadString('aparencia','ticket','');
  //Certificado
  caminhopfx:=ini.ReadString('certificado','caminho','');
  senhapfx:=ini.ReadString('certificado','senha','');
  seriepfx:=ini.ReadString('certificado','numserie','');
  ssllib  :=ini.ReadInteger('certificado','ssllib',0);
  crypt   :=ini.ReadInteger('certificado','cryptlib',0);
  httplib :=ini.ReadInteger('certificado','httplib',0);
  xmlsign :=ini.ReadInteger('certificado','XmlSignLib',0);
  //////////////////////////////////////////////////////////////////////////////
  // Geral
  //////////////////////////////////////////////////////////////////////////////
  IDToken :=ini.ReadString('geral','idtoken','');
  CSCToken:=ini.ReadString('geral','token','');
  atualizaxml:=ini.ReadBool('geral','atualizarxml',true);
  erroschema:=ini.ReadBool('geral','exibirerroschema',true);
  retiraacento:=ini.ReadBool('geral','retiraracentos',true);
  salvarxml:=ini.ReadBool('geral','salvar',true);
  alerta:=ini.ReadString('geral','FormatoAlerta','');
  pathsalvar:=ini.ReadString('geral','pathsalvar','');
  pathschemas:=ini.ReadString('geral','pathschemas','');
  //////////////////////////////////////////////////////////////////////////////
  // WebServices
  //////////////////////////////////////////////////////////////////////////////
  ambiente    := ini.ReadString('webservice','ambiente','');
  ufambiente  := ini.ReadString('webservice','uf','');
  ssltype     := ini.ReadInteger('webservice','ssltype',0);
  aguardar    := ini.ReadString('webservice','aguardar','');
  tentativas  := ini.ReadString('webservice','tentativas','');
  intervalo   := ini.ReadString('webservice','intervalo','');
  timeout     := ini.ReadInteger('webservice','timeout',0);
  fusohorario := ini.ReadString('webservice','fusohorario','');
  if fusohorario='' then showmessage('Ateno'#13'Fuso Horrio no informado nas configuraes');
  //////////////////////////////////////////////////////////////////////////////
  // arquivos
  //////////////////////////////////////////////////////////////////////////////
  pathnfe     := ini.ReadString('arquivos','pathnfe','');
  pathevento  := ini.ReadString('arquivos','pathevento','');
  autorizado2 := ini.ReadString('arquivos','autorizado2','');
  // email
  hostmail    := ini.ReadString('email','host','');
  portmail    := ini.ReadString('email','port','');
  usermail    := ini.ReadString('email','user','');
  passmail    := ini.ReadString('email','pass','');
  xsslmail    := ini.ReadString('email','ssl','');

  ini.free;
   // ZEOS
  CONECTAWEB.Connected       := False;
  CONECTAWEB.HostName        := hostweb;
  CONECTAWEB.Database        := bancoweb;
  CONECTAWEB.User            := usuarioweb;
  //
 // Verifica se o arquivo do banco de dados existe
  if not FileExists(bdsys) then
  begin
  // O arquivo no existe, exiba uma mensagem de aviso
    ShowMessage('O arquivo de banco de dados no foi encontrado. A aplicao ser encerrada.');

    // Aguarde um momento para que o usurio veja a mensagem antes de fechar
    Sleep(3000); // Espere 3 segundos (pode ajustar o tempo conforme necessrio)

    // Feche a aplicao
    Halt; // Isso encerrar a aplicao imediatamente
  end
  else
  begin
    // O arquivo existe, continue com a conexo
    CONECTA.Connected := False;
    CONECTA.HostName := host;
    CONECTA.Port := porta;
    CONECTA.Protocol := banco;
    CONECTA.User := usuario;
    CONECTA.Password := senha;
    CONECTA.Database := bdsys;
  end;
  //
  seupedido.connected     :=false;
  {dm_dados.seupedido.hostname      :=hostmenuto;
  dm_dados.seupedido.database      :=bdmenuto;
  dm_dados.seupedido.user          :='seupedido_israel';}
  //showmessage('host '+hostmenuto);
  //showmessage('bd '+bdmenuto);

  if temnet=0 then
  begin
    try
      CONECTAWeb.connected:=true;
  except
    on E:Exception do
    MessageDlg('Erro ao conectar!'#13'Erro: ' + e.Message, mtError, [mbOK], 0);
  end
  end
  else
  begin
    showmessage('Sem Internet');
  end;


  if (hostmenuto<>'') and (bdmenuto<>'') then
  begin
    if (temnet=0)  then
    begin
    try
      seupedido.connected:=true;
    except
    on E:Exception do
      MessageDlg('Erro ao conectar!'#13'Erro: ' + e.Message, mtError, [mbOK], 0);
    end
    end
    else
    begin
      showmessage('Sem acesso seupedido');
    end;
  end;

  if (sync=3)then
  begin
    if bdintegra<>'' then
    begin
      try
        Integra.HostName:='localhost';
        Integra.Database:=bdintegra;
        Integra.Connected:=true;
      except
        on I:Exception do
        messagedlg('Erro ao Conectar a Integra'#13'Erro: ' + i.Message,mtError,[mbOK],0);
      end;
    end;
  end;

end;

end.
